    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bagisto Europe",
            "email": "info@bagisto.eu"
        }
    ],
    "support": {
        "issues": "https://github.com/bagisto-europe/vies/issues",
        "forum":  "https://forums.bagisto.com",
